html, body {
  font-size:13px !important;
}
/* Page load background colour */
.browserContainer {
  background:#000000 !important;
}
 
/* Tabs
 * *****/
#TabsToolbar {
  display:none;
}
 
/* Navigation
 * ***********/
#navigator-toolbox {
  border-bottom:1px solid #191919 !important;
  z-index:999 !important;
}
#forward-button, #back-button {
  display:none !important;
}
#nav-bar {
  color:#ffffff !important;
  background:#000000 !important;
}
#urlbar-container, #search-container {
  margin-inline:0 !important;
  background:#000000 !important;
  border-right:1px solid #191919 !important;
}
#urlbar-background {
  background-color:#000000 !important;
  box-shadow:none !important;
  border:none !important;
}
#urlbar-input-container {
  color:#ffffff !important;
}
.urlbarView {
  color:#ffffff !important;
}
.urlbarView-url {
  color:#1f75fe !important;
}
/* TODO: Figure out where actual radius is set */
#urlbar[breakout][breakout-extend] > * {
  border-radius:0 !important;
}
:root:not([uidensity="compact"]):not([chromehidden~="toolbar"]) #PanelUI-button {
  border-image:none !important;
  border-left:1px solid #191919 !important;
}
#reader-mode-button[readeractive], #pageAction-panel-bookmark[starred], #star-button[starred] {
  fill:#ffffff !important;
}
#pageActionSeparator {
  display:none !important;
}
#pocket-button {
  display:none !important;
}
#tracking-protection-icon-container {
  border:none !important;
}
#urlbar[breakout][breakout-extend] {
  border-bottom:1px solid #191919 !important;
  border-right:1px solid #191919 !important;
}
.urlbarView-body-inner {
  border-top:none !important;
}
.urlbarView-body-outer {
  border-bottom:1px solid #191919 !important;
}
.urlbarView-row {
  padding-block:0 !important;
}
.urlbar-View-body-outer {
  padding:3px !important;
}
.urlbarView-body-outer:hover > .urlbarView-row-inner, .urlbarView-row-inner:hover {
  border-radius:0 !important;
  background:#191919 !important;
}
/* Sidebar
---------- */
#sidebar-box {
  display:flex;
  width:30px;
  min-width:30px;
  max-width:30px;
  overflow:hidden;
  position:fixed;
  z-index:998;
  top:37px;
  bottom:0;
  left:0;
  transition:all ease 166ms;
  background:#000000 !important;
  border-right:1px solid #191919;
}
#sidebar-box:hover {
  width:260px;
  min-width:260px;
  max-width:260px;
}
#sidebar-box * {
  background:#000000 !important;
}
#sidebar {
  width:auto !important;
  min-width:auto !important;
  max-width:auto !important;
  flex:auto;
  background:#000000 !important;
}
#sidebar-splitter {
  display:none;
}
#sidebar-header {
  display:none;
}
#main-window #browser {
  margin-left:30px;
}
#main-window[inFullscreen] #browser {
  margin-left:0;
}
#main-window[inFullscreen] #sidebar-box {
  display: none !important;
}
/* Hide tab bar when only one window is open */
/* This relies on the Tab Count in Window Title addon for selection */
/* TODO: Refactor using sidebarcommand attribute selection */
#main-window[titlepreface^="[1] "] #sidebar-box[sidebarcommand="treestyletab_piro_sakura_ne_jp-sidebar-action"] {
  display:none !important;
}
#main-window[titlepreface^="[1] "] #browser {
  margin-left:0;
}
/* Context Menus
---------------- */
/*  */
popupset > * {
  background:000000 !important;
  font-weight:500 !important;
}
popupset {
  border-color:#191919 !important;
  box-shadow:none !important;
  background:#000000 !important;
  font-weight:500 !important;
}
popupset * {
  border-color:#191919 !important;
  box-shadow:none !important;
  font-weight:500 !important;
}
panelview {
  background:#000000 !important;
  border-color:#191919 !important;
}
panelview * {
  color:#ffffff !important;
}
.panel-arrowcontent {
  box-shadow:0 0 3px 0 rgba(0,0,0,0.309) !important;
  border:1px solid #191919 !important;
  background:#ffffff !important;
}
:-moz-any(panel, menupopup)::part(arrow) {
  fill:#ffffff !important;
}
menupopup {
  -moz-appearance: none !important;
  background-color: #000000 !important;
  border: 1px #191919 solid !important;
}
menupopup * {
  color:#ffffff !important;
  fill:#ffffff !important;
}
menuseparator {
  -moz-appearance: none !important;
  border-bottom:1px solid #191919 !important;
}
menuitem, menu {
  -moz-appearance: none !important;
  color:#ffffff !important;
  padding: 7.5px 0px 7.5px 0px !important;
}
menuitem[disabled="true"], menuitem[disabled="true"] > .menu-text {
  color:#ffffff !important;
}
menuitem:hover, menu:hover, menu[open="true"], menuitem[_moz-menuactive="true"]	 {
  -moz-appearance: none !important;
  padding: 7.5px 0px 7.5px 0px !important;
  background-color:#1f75fe !important;
}
menugroup {
  padding: 0px !important;
  background-color: #fffff1 !important;
}
menuitem > .menu-text, menuitem > .menu-iconic-text {
  color:#ffffff !important;
}
menuitem[type="checkbox"] {
  padding-left: 8px !important;
  padding-top: 6.5px !important;
  padding-bottom: 6.5px !important;
}
menuitem[type="radio"] {
  padding-left: 8px !important;
  padding-top: 6.5px !important;
  padding-bottom: 6.5px !important;
}
menuitem[type="checkbox"] > .menu-iconic-text, menuitem[type="radio"] > .menu-iconic-text {
  padding-left: 5px !important;
}
menuitem[checked="true"][type="checkbox"] > .menu-iconic-left {
  padding: 7px 0px 0px 0px !important;
  border: solid white;
  border-width: 0 0 2px 2px;
  transform: rotate(-45deg) scale(.75);
  margin-bottom: 4px !important;
}
menuitem[checked="true"][type="radio"] > .menu-iconic-left {
  padding: 1px !important;
  border: solid white;
  height: 17px !important;
  border-radius: 100%;
  background: white;
  transform: scale(.5);
}
menucaption {
  background-color: #fffff1 !important;
  color:#ffffff !important;
}
#ContentSelectDropdown > menupopup {
  background-image: none !important;
}
#ContentSelectDropdown {
  padding: 0px !important;
}
menulist {
  -moz-appearance: none !important;
  padding: 4px 4px 4px 4px !important;
  border: 2px solid #191919 !important;
  background-color: #fffff1 !important;
}
menulist:hover {
  padding: 4px 4px 4px 4px !important;
  border: 2px solid #191919 !important;
  background-color: #fffff1 !important;
}
menulist[open="true"]  {
  border: 2px solid #666666 !important;
  background-color: #000000 !important;
}
#context-back > .menu-iconic-left, #context-forward > .menu-iconic-left, #context-reload > .menu-iconic-left, #context-stop > .menu-iconic-left, #context-bookmarkpage > .menu-iconic-left {
  padding: 0px !important;
}
.menu-iconic-left {
  -moz-appearance: none !important;
  padding: 0px 5px 0px 9px !important;
}
.menu-right {
  -moz-appearance: none !important;
  padding: 7.5px !important;
  margin-right: 6px !important;
  color:#ffffff !important;
  border:1px solid #ffffff;
  border-width: 0 2px 2px 0px;
  transform: rotate(-45deg) scale(.55);
}
tooltip {
  -moz-appearance: none !important;
  color:#ffffff !important;
  background-color: #000000 !important;
  border: 1px solid #191919 !important;
  padding: 6.5px !important;
}
#context-navigation {
  background:#000000 !important;
}
#context-navigation * {
  fill:#F5D3A9 !important;
}
#context-navigation *:hover {
  background:#000000 !important;
}
.popup-notification-body-container {
  color:#ffffff !important;
  background:#000000 !important;
}
